+week    7;19971201;4
+%fvg:abbrev not confirmed
+abmon   "sch"; "fav"; "mar"; "avr"; "mat"; "zer";/
+        "fan"; "avu"; "set"; "oct"; "nov"; "dec"
+mon     "schaner"; "favrer"; "mars";/
+        "avrigl"; "matg"; "zercladur";/
+        "fanadur"; "avust"; "settember";/
+        "october"; "november"; "december"
