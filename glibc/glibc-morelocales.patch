+LC_NUMERIC
+decimal_point             "<U002E>"
+thousands_sep             "<U0020>"	%fvg:dito
+grouping                  3;3
+END LC_NUMERIC
+
+LC_TIME
+%fvg:dictionary says: mixed two-char/three-char abbrev:
+%abday   "du"; "gli"; "ma"; "me"; "gie"; "ve"; "so"
+%but specification wants "of equal length" !
+abday   "dum"; "gli"; "mar"; "mes"; "gie"; "ven"; "son"
